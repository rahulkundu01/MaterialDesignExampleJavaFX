      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane fx:id="pane" prefHeight="203.0" prefWidth="400.0">
         <children>
            <SplitPane layoutX="15.0" layoutY="11.0" prefHeight="180.0" prefWidth="373.0">
               <items>
                  <Pane prefHeight="176.0" prefWidth="371.0">
                     <children>
                        <JFXColorPicker fx:id="cpSelect" layoutX="186.0" layoutY="71.0" onAction="#changeColor" />
